<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="sureshText">
        <attr name="font_name">
            <enum name="regular" value="1" />
            <enum name="bold" value="2" />
            <enum name="semibold" value="3" />
            <enum name="heavy" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ArivistaRadioButton">
        <attr name="lrb_innerCircleRadius" format="dimension"/>
        <attr name="lrb_strokeRadius" format="dimension"/>
        <attr name="lrb_strokeWidth" format="dimension"/>
        <attr name="lrb_outterPadding" format="dimension"/>
        <attr name="lrb_explodeCount" format="integer"/>
        <attr name="lrb_inAnimDuration" format="integer"/>
        <attr name="lrb_outAnimDuration" format="integer"/>
        <attr name="lrb_checkedColor" format="color"/>
        <attr name="lrb_unCheckedColor" format="color"/>
    </declare-styleable>
</resources>